<Window x:Class="AccessDb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccessDb"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="See_Assets" Content="See Assets" HorizontalAlignment="Left" Margin="10,144,0,0" VerticalAlignment="Top" Width="89" Height="31" Click="See_Assets_Click"/>
        <TextBox x:Name="AssetsTextBox" HorizontalAlignment="Left" Height="67" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="214" Margin="10,53,0,0" TextChanged="AssetsTextBox_TextChanged"/>
    </Grid>
</Window>
